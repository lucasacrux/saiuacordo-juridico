## Backend Raspado – Exemplo com valores
PORT=8000
FRONT_ORIGIN=http://localhost:5175
COOKIE_DOMAIN=
NODE_ENV=development
AUTH_SECRET=troque-este-segredo
HEADLESS=0
PW_SLOWMO=150
TJSP_OTP_TIMEOUT_MS=30000
TJSP_FLOW_TTL_MS=1800000

# Supabase (NÃO use chaves reais em exemplos públicos)
SUPABASE_URL=https://example.supabase.co
SUPABASE_SERVICE_ROLE_KEY=chave-de-exemplo

# MySQL PRD (apenas para ingest local – use com cuidado)
DB_HOST=127.0.0.1
DB_USER=usuario
DB_PASSWORD=senha
DB_NAME=database
DB_PORT=3306

# Credenciais TJSP (opcionais para automação controlada)
TJSP_USER=000.000.000-00
TJSP_PASSWORD=senha
TJSP_URL=https://esaj.tjsp.jus.br/sajcas/login

# 2FA por e-mail (IMAP; ex: Gmail)
ENABLE_2FA_EMAIL=0
MAIL_IMAP_HOST=imap.gmail.com
MAIL_IMAP_PORT=993
MAIL_IMAP_SECURE=1
MAIL_IMAP_USER=usuario@example.com
MAIL_IMAP_PASS=senha-ou-app-password
MAIL_LOOKBACK_MINUTES=15
MAIL_OTP_FROM=noreply@tjsp.jus.br
MAIL_OTP_SUBJECT_RX=(c[oó]digo|verifica[cç][aã]o|autentica[cç][aã]o)
MAIL_OTP_BODY_RX=(\b\d{6}\b)

# Depuração Playwright
PW_PERSIST_SESSION="1"
PW_USER_DATA_DIR=".pw-esaj"
PW_KEEP_OPEN="0"
PW_STORAGE_STATE="esaj-state.json"
